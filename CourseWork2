num = int(input("input a factorial number:")) 

if num==0: 
    print("one zero!") 
elif num<0:
    print("not a valid number")
else :
    total = 0
    five = 0
 
    for i in range(1,num+1):
        if i%5==0:
            while i%5 == 0 and i != 1: 
                i = i/5 
                five = five+1
  
    print(five)
